---
interface Props {
  src: string;
  id: string;
}

const { src, id } = Astro.props as Props;
---

<div class="p-4">
  <a href={`/skills/${id}`}>
    <img
      transition:name={id}
      class="rounded-xl transition hover:scale-110"
      width="100"
      height="100"
      src={src}
    />
  </a>
</div>
